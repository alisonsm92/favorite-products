{
	"env": {
		"jest": true,
		"browser": true,
		"es2021": true
	},
	"extends": [
		"airbnb-base",
		"plugin:jest/recommended"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": [
		"@typescript-eslint",
		"jest"
	],
	"rules": {
		"indent": ["error", 4]
	}
}
